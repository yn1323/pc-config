{
    "title": "ボリュームをズームに変更",
    "maintainers": [
      "yn1323"
    ],
    "rules": [
    {
    "description": "ボリュームDownでズームアウトに変更",
    "manipulators": [
      {
        "type": "basic",
        "from": {
          "key_code": "volume_decrement",
          "modifiers": {
            "mandatory": [
              "left_command"
            ]
          }
        },
        "to": [
          {
            "key_code": "hyphen",
            "modifiers": [
              "left_shift",
              "left_command"
            ]
          }
        ]
      }
    ]
    },
    {
    "description": "ボリュームUpでズームインに変更",
    "manipulators": [
      {
        "type": "basic",
        "from": {
          "key_code": "volume_increment",
          "modifiers": {
            "mandatory": [
              "left_command"
            ]
          }
        },
        "to": [
          {
            "key_code": "equal_sign",
            "modifiers": [
              "left_shift",
              "left_command"
            ]
          }
        ]
      }
    ]
    },
    {
    "description": "Chromeズームリセット",
    "manipulators": [
      {
        "type": "basic",
        "from": {
          "key_code": "mute"
        },
        "to": [
          {
            "key_code": "0",
            "modifiers": [
              "left_command"
            ]
          }
        ]
      }
    ]
    }
  ]
}